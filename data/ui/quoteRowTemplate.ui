<interface>
  <template class="QuoteRowTemplate" parent="GtkListBoxRow">
    <property name="name">quoteRowTemplate</property>
    <child>
      <object class="GtkBox">
        <child>
          <object class="GtkLabel" id="quoteName">
            <property name="name">rowQuoteName</property>
            <property name="hexpand">true</property>
            <property name="xalign">0</property>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="quoteSymbol">
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
